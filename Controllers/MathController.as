package  {	import flash.display.MovieClip;	import flash.display.DisplayObject;		import flash.geom.Point;	import flash.events.*;	public class MathController {		public function MathController() {			// constructor code		}				public static function roundTo(num:Number, decimals:int):Number{			//if decimals is 0, round to 0 decimal places			//if decimals is 1, round to 1 decimal place			//if decimal is -1, round to the nearest tenth			return Math.round(num *Math.pow(10, decimals)) / Math.pow(10, decimals);		}				public static function distBetween(mc1, mc2):Number{			//REQUIRES: mc1 and mc2 must be either DisplayObject or Point type objects			//PROMISES: Returns the absolute distance between mc1 and mc2						if((!(mc1 is DisplayObject) && !(mc1 is Point)) || (!(mc2 is DisplayObject) && !(mc2 is Point))){				trace("WARNING: mc1 or/and mc2 are neither MovieClips or Points.");			}						return Math.sqrt(Math.pow(mc1.x - mc2.x, 2) + Math.pow(mc1.y - mc2.y, 2));		}	}	}